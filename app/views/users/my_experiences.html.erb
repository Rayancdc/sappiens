<div class="container"><h1 class="text-center mobile-bookings">My Experiences</h1>
 <h3 class="text-center"><%= link_to "Create new experiences", new_experience_path %></h3>
 </div>
  <div class="container">
     <div class="row">
       <% @experiences.each do |experience| %>
         <div class="col-xs-12 col-sm-6">
           <%= link_to experience_path(experience) do  %>
             <div class="card-my-bookings">
               <div class="card-description-my-bookings">
               <h2><%= experience.career %></h2>
               <p>Address: <%= experience.company.address %></p>
               </div>
               <div class= "ratings-my">
                 <% 4.times do %>
                 <%# (booking.rating).times do %>
                   <span class="fa fa-star checked"></span>
                 <% end %>
                 <%# (5 - booking.rating).times do %>
                   <span class="fa fa-star"></span>
                 <%# end %>
               </div>
             </div>
        <% end %>
       </div>
     <% end %>
   </div>
 </div>


